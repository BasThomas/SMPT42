<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true"
        android:right="-5dp"
        android:left="-5dp"
        android:top="-5dp">
        <shape android:shape="rectangle" >
            <solid
                android:color="#ff8080" />
            <stroke
                android:width="1dp"
                android:color="#ff0000" />
        </shape>
    </item>
    <item
        android:bottom="-5dp"
        android:top="-5dp">
        <shape android:shape="rectangle" >
            <solid
                android:color="#ff8080" />
            <stroke
                android:width="1dp"
                android:color="#ff0000" />
        </shape>
    </item>
</layer-list>
