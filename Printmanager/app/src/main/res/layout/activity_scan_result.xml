<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="smpt42.nl.printmanager.view.ScanResultActivity" >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="[NAME]"
        android:id="@+id/textViewName"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:id="@+id/textViewName2"
        android:layout_below="@+id/textViewName"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="45dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Company"
        android:id="@+id/textViewCompany"
        android:layout_below="@+id/textViewName2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adres"
        android:id="@+id/textViewAdres"
        android:layout_below="@+id/textViewCompany"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tel"
        android:id="@+id/textViewTel"
        android:layout_below="@+id/textViewAdres"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Scanned Date"
        android:id="@+id/textViewScannedDate"
        android:layout_below="@+id/textViewTel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Printed Date"
        android:id="@+id/textViewPrintedDate"
        android:layout_below="@+id/textViewScannedDate"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewNameValue"
        android:layout_above="@+id/textViewCompany"
        android:layout_alignLeft="@+id/textViewCompanyValue"
        android:layout_alignStart="@+id/textViewCompanyValue" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewCompanyValue"
        android:layout_below="@+id/textViewNameValue"
        android:layout_alignLeft="@+id/textViewAdresValue"
        android:layout_alignStart="@+id/textViewAdresValue" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewAdresValue"
        android:layout_below="@+id/textViewCompanyValue"
        android:layout_alignLeft="@+id/textViewTelValue"
        android:layout_alignStart="@+id/textViewTelValue" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewTelValue"
        android:layout_below="@+id/textViewAdresValue"
        android:layout_alignLeft="@+id/textViewScannedDateValue"
        android:layout_alignStart="@+id/textViewScannedDateValue" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewScannedDateValue"
        android:layout_marginLeft="20dp"
        android:layout_alignTop="@+id/textViewScannedDate"
        android:layout_toRightOf="@+id/textViewScannedDate"
        android:layout_toEndOf="@+id/textViewScannedDate" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewPrintedDateValue"
        android:layout_below="@+id/textViewScannedDateValue"
        android:layout_alignLeft="@+id/textViewScannedDateValue"
        android:layout_alignStart="@+id/textViewScannedDateValue" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listViewTasks"
        android:layout_marginTop="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/textViewPrintedDate" />
</RelativeLayout>
